{
  "name": "cruijff",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:beta": "cd ./android && fastlane beta",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:clean": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:promote": "code-push promote ChainReactConf-Android Staging Production -t '*'",
    "android:push:staging": "code-push release-react SportySpotsAndroid Android -d Staging",
    "android:push:production": "code-push release-react SportySpotsAndroid Android -d Production",
    "android:run": "react-native run-android",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "fixcode": "standard --fix",
    "git-hook": "npm run lint -s && npm run test -s",
    "ios:beta": "cd ./ios && fastlane beta",
    "ios:bundle": "react-native bundle --entry-file index.ios.js --platform ios --dev false --reset-cache --bundle-output ./ios/main.jsbundle --assets-dest ./ios/release/main.jsbundle",
    "ios:promote": "code-push promote ChainReactConf Staging Production -t '*'",
    "ios:push:staging:": "code-push release-react SportySpots iOS -d Staging",
    "ios:push:production:": "code-push release-react SportySpots iOS -d Production",
    "ios:run": "react-native run-ios",
    "ios:tryRelease": "react-native run-ios --configuration Release",
    "jest:updateSnapshot": "jest --updateSnapshot",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "precommit": "npm test",
    "prepush": "npm run lint -s && npm run test -s",
    "push:history": "echo 'IOS Production History' && code-push deployment history ChainReactConf Production && echo 'Android Production History' && code-push deployment history ChainReactConf-Android Production",
    "push:status": "echo 'IOS' && code-push deployment ls ChainReactConf && echo 'Android' && code-push deployment ls ChainReactConf-Android",
    "push": "yarn run ios:push && yarn run android:push",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "storybook": "storybook start -p 7007",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "dependencies": {
    "apisauce": "0.14.1",
    "format-json": "1.0.3",
    "lodash": "4.17.4",
    "mobile-center": "0.9.0",
    "mobile-center-analytics": "0.9.0",
    "mobile-center-crashes": "0.9.0",
    "prop-types": "15.5.10",
    "querystringify": "1.0.0",
    "ramda": "0.24.1",
    "react": "16.0.0-alpha.12",
    "react-native": "0.48.3",
    "react-native-animatable": "1.2.4",
    "react-native-config": "0.6.1",
    "react-native-device-info": "0.11.0",
    "react-native-drawer": "2.5.0",
    "react-native-i18n": "2.0.6",
    "react-native-material-ui": "1.13.0",
    "react-native-offline": "3.1.1",
    "react-native-svg": "5.4.1",
    "react-native-vector-icons": "4.4.0",
    "react-navigation": "1.0.0-beta.11",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-persist": "4.9.1",
    "redux-saga": "0.15.6",
    "reduxsauce": "0.6.0",
    "seamless-immutable": "7.1.2"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "3.2.8",
    "@storybook/react-native": "3.2.8",
    "babel-eslint": "8.0.0",
    "babel-jest": "21.0.2",
    "babel-plugin-ignite-ignore-reactotron": "0.3.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react-native": "4.0.0",
    "code-push-cli": "2.1.0-beta",
    "enzyme": "2.9.1",
    "eslint-plugin-flowtype": "2.35.1",
    "flow-bin": "0.55.0",
    "husky": "0.14.3",
    "ignite-animatable": "1.0.0",
    "ignite-dev-screens": "2.1.0",
    "ignite-i18n": "1.1.0",
    "ignite-ir-boilerplate": "2.1.1",
    "ignite-standard": "1.0.0",
    "ignite-vector-icons": "1.1.0",
    "jest": "21.1.0",
    "mockery": "2.1.0",
    "react-addons-test-utils": "15.6.0",
    "react-dom": "16.0.0-alpha.12",
    "react-test-renderer": "16.0.0-alpha.12",
    "reactotron-react-native": "1.12.3",
    "reactotron-redux": "1.12.2",
    "reactotron-redux-saga": "1.12.2",
    "snazzy": "7.0.0",
    "standard": "10.0.3"
  },
  "jest": {
    "testMatch": [
      "**/Tests/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "Tests/Setup.js"
    ],
    "setupFiles": [
      "./Tests/Setup"
    ],
    "preset": "react-native"
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  },
  "rnpm": {
    "assets": [
      "fonts"
    ]
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.2"
  },
  "optionalDependencies": {
    "npm-check": "5.4.5"
  }
}
